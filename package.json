{
  "name": "oe-generator",
  "version": "1.2.2",
  "description": "Rails-inspired generator system that provides scaffolding for your apps",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "git@github.com:EdgeVerve/oe-generator.git"
  },
  "author": "Yeoman",
  "main": "lib",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "xo && gulp",
    "doc": "jsdoc -c jsdoc.json"
  },
  "files": [
    "lib"
  ],
  "keywords": [
    "cli",
    "scaffold",
    "scaffolding",
    "generate",
    "generator",
    "app"
  ],
  "dependencies": {
    "async": "2.6.3",
    "chalk": "1.1.3",
    "cli-table": "0.3.1",
    "cross-spawn": "5.1.0",
    "dargs": "5.1.0",
    "dateformat": "2.2.0",
    "debug": "2.6.9",
    "detect-conflict": "1.0.1",
    "error": "7.2.1",
    "find-up": "2.1.0",
    "github-username": "4.1.0",
    "istextorbinary": "2.6.0",
    "lodash": "4.17.15",
    "mem-fs-editor": "6.0.0",
    "minimist": "1.2.0",
    "mkdirp": "0.5.1",
    "pretty-bytes": "4.0.2",
    "read-chunk": "2.1.0",
    "read-pkg-up": "2.0.0",
    "rimraf": "2.7.1",
    "run-async": "2.3.0",
    "shelljs": "0.7.8",
    "text-table": "0.2.0",
    "through2": "2.0.5",
    "oe-environment": "git+http://evgit/oecloud.io/oe-environment.git#1.6.7"
  },
  "devDependencies": {
    "gulp": "4.0.2",
    "gulp-mocha": "7.0.2",
    "gulp-plumber": "1.2.1",
    "inquirer": "6.5.2",
    "jsdoc": "3.6.3",
    "mockery": "2.1.0",
    "nock": "9.6.1",
    "proxyquire": "1.8.0",
    "sinon": "2.4.1",
    "tui-jsdoc-template": "1.2.2",
    "xo": "0.25.3",
    "yeoman-assert": "3.1.1",
    "yeoman-test": "1.9.1"
  },
  "xo": {
    "space": true,
    "rules": {
      "promise/no-callback-in-promise": "off"
    },
    "overrides": [
      {
        "files": "test/**",
        "envs": [
          "node",
          "mocha"
        ]
      }
    ]
  }
}
